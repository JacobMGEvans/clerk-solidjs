{
  "name": "clerk-solidjs",
  "description": "Clerk for SolidJS",
  "version": "0.2.1",
  "keywords": [
    "solidjs",
    "clerk",
    "solid",
    "clerk-auth"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ian-pascoe/clerk-solidjs"
  },
  "license": "MIT",
  "author": "Ian Pascoe",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "dev:publish": "bun run dev -- --env.publish",
    "format": "prettier --write .",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "prepare": "bun run build",
    "changeset": "changeset",
    "prerelease": "bun run lint && bun run test",
    "release": "changeset publish",
    "clean": "rimraf dist",
    "publish:local": "bunx yalc push --replace  --sig"
  },
  "dependencies": {
    "@clerk/backend": "^1.3.1",
    "@clerk/clerk-js": "^5.8.1",
    "@clerk/shared": "^2.3.1",
    "@solid-primitives/context": "^0.2.3",
    "@solid-primitives/destructure": "^0.1.17",
    "@solid-primitives/memo": "^1.3.9",
    "@solid-primitives/static-store": "^0.0.8",
    "@solidjs/testing-library": "^0.8.8",
    "@tanstack/solid-query": "^5.51.2",
    "prettier-plugin-organize-imports": "^4.0.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.7",
    "@clerk/themes": "^2.1.10",
    "@clerk/types": "^4.6.1",
    "@eslint/js": "^9.6.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/user-event": "^14.5.2",
    "changeset": "^0.2.6",
    "eslint": "^9.6.0",
    "expect-type": "^0.19.0",
    "globals": "^15.8.0",
    "jsdom": "^24.1.0",
    "prettier": "3.3.2",
    "rimraf": "^6.0.1",
    "tsup": "^8.1.0",
    "tsup-preset-solid": "^2.2.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.0",
    "vite-plugin-solid": "^2.10.2",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.0.2"
  },
  "peerDependencies": {
    "solid-js": ">=1",
    "@solidjs/start": ">=1"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.1.17",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index/index.cjs",
  "module": "./dist/index/index.js",
  "types": "./dist/index/index.d.ts",
  "browser": {
    "./dist/server/server.js": "./dist/server/index.js",
    "./dist/server/server.cjs": "./dist/server/index.cjs"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index/index.d.ts",
        "default": "./dist/index/index.js"
      },
      "require": {
        "types": "./dist/index/index.d.cts",
        "default": "./dist/index/index.cjs"
      }
    },
    "./errors": {
      "import": {
        "types": "./dist/errors/index.d.ts",
        "default": "./dist/errors/index.js"
      },
      "require": {
        "types": "./dist/errors/index.d.cts",
        "default": "./dist/errors/index.cjs"
      }
    },
    "./server": {
      "worker": {
        "import": {
          "types": "./dist/server/index.d.ts",
          "default": "./dist/server/server.js"
        },
        "require": {
          "types": "./dist/server/index.d.cts",
          "default": "./dist/server/server.cjs"
        }
      },
      "browser": {
        "import": {
          "types": "./dist/server/index.d.ts",
          "default": "./dist/server/index.js"
        },
        "require": {
          "types": "./dist/server/index.d.cts",
          "default": "./dist/server/index.cjs"
        }
      },
      "deno": {
        "import": {
          "types": "./dist/server/index.d.ts",
          "default": "./dist/server/server.js"
        },
        "require": {
          "types": "./dist/server/index.d.cts",
          "default": "./dist/server/server.cjs"
        }
      },
      "node": {
        "import": {
          "types": "./dist/server/index.d.ts",
          "default": "./dist/server/server.js"
        },
        "require": {
          "types": "./dist/server/index.d.cts",
          "default": "./dist/server/server.cjs"
        }
      },
      "import": {
        "types": "./dist/server/index.d.ts",
        "default": "./dist/server/index.js"
      },
      "require": {
        "types": "./dist/server/index.d.cts",
        "default": "./dist/server/index.cjs"
      }
    }
  },
  "typesVersions": {
    "*": {
      "errors": [
        "./dist/errors/index.d.ts"
      ],
      "server": [
        "./dist/server/index.d.ts"
      ]
    }
  }
}
