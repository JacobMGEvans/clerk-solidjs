{
  "name": "clerk-solidjs",
  "description": "Clerk for SolidJS",
  "version": "0.2.4",
  "keywords": [
    "solidjs",
    "clerk",
    "solid",
    "clerk-auth"
  ],
  "bugs": {
    "url": "https://github.com/ian-pascoe/clerk-solidjs/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ian-pascoe/clerk-solidjs"
  },
  "license": "MIT",
  "author": "Ian Pascoe",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts",
        "dist/server.d.ts",
        "dist/errors.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "solid": "./dist/index.jsx",
      "default": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "default": "./dist/server.js"
    },
    "./errors": {
      "types": "./dist/errors.d.ts",
      "default": "./dist/errors.js"
    },
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "source": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "dev:publish": "bun run dev -- --env.publish",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "prepare": "bun run build",
    "changeset": "changeset",
    "prerelease": "bun run lint && bun run test",
    "version": "changeset version && bun install --no-frozen-lockfile",
    "release": "changeset publish",
    "clean": "rimraf dist",
    "publish:local": "bunx yalc push --replace  --sig"
  },
  "dependencies": {
    "@clerk/backend": "^1.3.1",
    "@clerk/clerk-js": "^5.8.1",
    "@clerk/shared": "^2.3.1",
    "@solid-primitives/context": "^0.2.3",
    "@solid-primitives/destructure": "^0.1.17",
    "@solid-primitives/memo": "^1.3.9",
    "@solid-primitives/static-store": "^0.0.8",
    "@solidjs/testing-library": "^0.8.8",
    "@tanstack/solid-query": "^5.51.2",
    "prettier-plugin-organize-imports": "^4.0.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.7",
    "@clerk/themes": "^2.1.10",
    "@clerk/types": "^4.6.1",
    "@eslint/js": "^9.6.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/user-event": "^14.5.2",
    "changeset": "^0.2.6",
    "esbuild-plugin-solid": "^0.6.0",
    "eslint": "^9.6.0",
    "expect-type": "^0.19.0",
    "globals": "^15.8.0",
    "jsdom": "^24.1.0",
    "prettier": "3.3.2",
    "rimraf": "^6.0.1",
    "tsup": "^8.1.0",
    "tsup-preset-solid": "^2.2.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.0",
    "vite-plugin-solid": "^2.10.2",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.0.2"
  },
  "peerDependencies": {
    "solid-js": "^1.8",
    "@solidjs/start": "^1"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.1.17",
  "publishConfig": {
    "access": "public"
  },
  "trustedDependencies": [
    "@clerk/shared",
    "browser-tabs-lock",
    "core-js"
  ]
}
