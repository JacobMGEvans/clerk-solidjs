{
  "name": "clerk-solid",
  "description": "Clerk for SolidJS",
  "version": "0.0.1",
  "keywords": [
    "solidjs",
    "clerk",
    "solid"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ian-pascoe/clerk-solid"
  },
  "license": "MIT",
  "author": "Ian Pascoe",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./errors": {
      "import": {
        "types": "./dist/errors.d.mts",
        "default": "./dist/errors.mjs"
      },
      "require": {
        "types": "./dist/errors.d.ts",
        "default": "./dist/errors.js"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "format": "prettier --write .",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "prepare": "bun run build",
    "changeset": "changeset",
    "prerelease": "bun run lint && bun run test",
    "release": "changeset publish",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@clerk/backend": "^1.3.1",
    "@clerk/clerk-js": "^5.7.2",
    "@clerk/shared": "^2.3.1",
    "@solid-primitives/context": "^0.2.3",
    "@solid-primitives/destructure": "^0.1.17",
    "@solid-primitives/memo": "^1.3.9",
    "@solid-primitives/static-store": "^0.0.8",
    "@solidjs/testing-library": "^0.8.8",
    "@tanstack/solid-query": "^5.51.2",
    "prettier-plugin-organize-imports": "^4.0.0"
  },
  "devDependencies": {
    "@clerk/themes": "^2.1.10",
    "@clerk/types": "^4.6.1",
    "@eslint/js": "^9.6.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/user-event": "^14.5.2",
    "changeset": "^0.2.6",
    "eslint": "9.x",
    "expect-type": "^0.19.0",
    "globals": "^15.8.0",
    "jsdom": "^24.1.0",
    "prettier": "3.3.2",
    "rimraf": "^6.0.1",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.0",
    "vite-plugin-solid": "^2.10.2",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.0.2"
  },
  "peerDependencies": {
    "solid-js": ">=1",
    "@solidjs/start": ">=1"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.1.17",
  "publishConfig": {
    "access": "public"
  }
}
